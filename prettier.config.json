{
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"experimentalTernaries": false,
	"insertPragma": false,
	"plugins": ["prettier-plugin-xml"],
	"printWidth": 120,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"semi": true,
	"singleAttributePerLine": false,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "all",
	"useTabs": true
}
